<ContentDialog
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:jpg_dump"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:IO="using:System.IO" x:Name="JpgDumpContent"
    x:Class="jpg_dump.ContentDialog1"
    mc:Ignorable="d"
    Title="JPG Loader"
    PrimaryButtonText="Select File"
    SecondaryButtonText="Exit"
    PrimaryButtonClick="ContentDialog_PrimaryButtonClick"
    SecondaryButtonClick="ContentDialog_SecondaryButtonClick" BorderThickness="0,0,0,5" RequestedTheme="Dark" BorderBrush="#FFD71400">
    <Grid x:Name="GridBirdInfo" HorizontalAlignment="Left" Height="523" VerticalAlignment="Top" Width="295">
        <Image x:Name="ImageBird" HorizontalAlignment="Left" Height="274" Margin="10,73,0,0" VerticalAlignment="Top" Width="275"/>
        <Hub x:Name="HubBirdInfo" Header="Hub" HorizontalAlignment="Left" Height="118" Margin="10,352,0,0" VerticalAlignment="Top" Width="275">
            <HubSection Header="HubSection 0">
                <DataTemplate>
                    <Grid/>
                </DataTemplate>
            </HubSection>
            <HubSection Header="HubSection 1">
                <DataTemplate>
                    <Grid/>
                </DataTemplate>
            </HubSection>
        </Hub>
    </Grid>
</ContentDialog>
